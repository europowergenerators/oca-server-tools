<odoo>

    <record forcecreate="True" id="config_sentry_enabled" model="ir.config_parameter">
        <field name="key">sentry.sentry_enabled</field>
        <field name="value">false</field>
    </record>
    <record forcecreate="True" id="config_sentry_dsn" model="ir.config_parameter">
        <field name="key">sentry.sentry_dsn</field>
        <field name="value">false</field>
    </record>
    <record
        forcecreate="True"
        id="config_sentry_logging_level"
        model="ir.config_parameter"
    >
        <field name="key">sentry.sentry_logging_level</field>
        <field name="value">warn</field>
    </record>
    <record
        forcecreate="True"
        id="config_sentry_exclude_loggers"
        model="ir.config_parameter"
    >
        <field name="key">sentry.sentry_exclude_loggers</field>
        <field name="value">werkzeug</field>
    </record>
    <record
        forcecreate="True"
        id="config_sentry_ignore_exceptions"
        model="ir.config_parameter"
    >
        <field name="key">sentry.sentry_ignore_exceptions</field>
        <field
            name="value"
        >odoo.exceptions.AccessDenied,odoo.exceptions.AccessError,odoo.exceptions.MissingError,odoo.exceptions.RedirectWarning,odoo.exceptions.UserError,odoo.exceptions.ValidationError,odoo.exceptions.Warning,odoo.exceptions.except_orm</field>
    </record>
    <record
        forcecreate="True"
        id="config_sentry_traces_sample_rate"
        model="ir.config_parameter"
    >
        <field name="key">sentry.sentry_traces_sample_rate</field>
        <field name="value">1</field>
    </record>
    <record
        forcecreate="True"
        id="config_sentry_include_context"
        model="ir.config_parameter"
    >
        <field name="key">sentry.sentry_include_context</field>
        <field name="value">true</field>
    </record>
    <record
        forcecreate="True"
        id="config_sentry_environment"
        model="ir.config_parameter"
    >
        <field name="key">sentry.sentry_environment</field>
        <field name="value">test</field>
    </record>
    <record forcecreate="True" id="config_sentry_odoo_dir" model="ir.config_parameter">
        <field name="key">sentry.sentry_odoo_dir</field>
        <field
            name="value"
        >/opt/odoo/buildouts/europowergenerators17/parts/odoo/odoo</field>
    </record>

</odoo>
